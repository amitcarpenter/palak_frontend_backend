          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify({
            amount: 500,
          }),
        });

        const order = await response.json();

        const options = {
          key: "rzp_test_XGkfx84OsbbVDp",
          amount: order.amount,
          currency: order.currency,
          name: "Amit Hello",
          description: "Purchase Description",
          image: "your_logo_url",
          order_id: order.id,
